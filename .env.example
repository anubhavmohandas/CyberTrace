# ============================================================
# CYBERTRACE CONFIGURATION
# ============================================================
# Copy this file to .env and fill in your API keys
# All keys are optional - CyberTrace works without them
# but having keys enables additional features and higher rate limits

# ==================== API KEYS ====================
# These are all FREE tier API keys

# VirusTotal - https://virustotal.com
# Sign up and get API key from your profile
# Free tier: 500 requests/day
VIRUSTOTAL_API_KEY=

# Shodan - https://shodan.io
# Sign up and get API key from My Account
# Free tier: 100 queries/month
SHODAN_API_KEY=

# URLScan - https://urlscan.io
# Sign up and get API key from Settings
# Free tier: 5000 scans/day
URLSCAN_API_KEY=

# GitHub - https://github.com/settings/tokens
# Generate a personal access token (no scopes needed for public data)
# Free tier: 5000 requests/hour
GITHUB_TOKEN=

# EmailRep - https://emailrep.io
# Sign up for free API key
# Free tier: 100 queries/day
EMAILREP_API_KEY=

# IntelligenceX - https://intelx.io
# Sign up for free API key
# Free tier: Limited queries
INTELX_API_KEY=

# Hunter.io - https://hunter.io
# Sign up for free API key
# Free tier: 25 verifications/month
HUNTER_API_KEY=

# NumVerify - https://numverify.com
# Sign up for free API key
# Free tier: 100 lookups/month
NUMVERIFY_API_KEY=

# Etherscan - https://etherscan.io
# Sign up for free API key
# Free tier: 5 calls/second
ETHERSCAN_API_KEY=

# ==================== TELEGRAM ====================
# For Telegram OSINT features (optional)
TELEGRAM_BOT_TOKEN=

# ==================== TOR CONFIGURATION ====================
# Enable Tor for direct .onion access (requires Tor service running)
TOR_ENABLED=false
TOR_SOCKS_HOST=127.0.0.1
TOR_SOCKS_PORT=9050
TOR_CONTROL_PORT=9051
TOR_PASSWORD=

# ==================== GENERAL SETTINGS ====================
# Cache time-to-live in hours (default: 24)
CACHE_TTL_HOURS=24

# HTTP request timeout in seconds (default: 30)
REQUEST_TIMEOUT=30

# Maximum concurrent requests (default: 10)
MAX_CONCURRENT=10

# ==================== CAPTCHA SERVICES ====================
# For automated Indian portal lookups (Vahan, etc.)
# 2Captcha - https://2captcha.com (~$2-3 per 1000 captchas)
TWOCAPTCHA_API_KEY=
